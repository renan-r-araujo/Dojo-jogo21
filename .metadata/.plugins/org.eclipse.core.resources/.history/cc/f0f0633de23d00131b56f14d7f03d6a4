package com.jogocartas.testes;

public static class GeradorStringEsperada {

	public static String baralhoOrdenado() {
		StringBuilder retorno = new StringBuilder();
		retorno.append("Às de Copas, 2 de Copas, 3 de Copas, 4 de Copas, ");
		retorno.append("5 de Copas, 6 de Copas, 7 de Copas, 8 de Copas, 9 de Copas, ");
		retorno.append("10 de Copas, Valete de Copas, Dama de Copas, Rei de Copas, ");
		retorno.append("Às de Espadas, 2 de Espadas, 3 de Espadas, 4 de Espadas, ");
		retorno.append("5 de Espadas, 6 de Espadas, 7 de Espadas, 8 de Espadas, ");
		retorno.append("9 de Espadas, 10 de Espadas, Valete de Espadas, Dama de Espadas, ");
		retorno.append("Rei de Espadas, Às de Ouros, 2 de Ouros, 3 de Ouros, 4 de Ouros, ");
		retorno.append("5 de Ouros, 6 de Ouros, 7 de Ouros, 8 de Ouros, 9 de Ouros, ");
		retorno.append("10 de Ouros, Valete de Ouros, Dama de Ouros, Rei de Ouros, ");
		retorno.append("Às de Paus, 2 de Paus, 3 de Paus, 4 de Paus, 5 de Paus, 6 de Paus, ");
		retorno.append("7 de Paus, 8 de Paus, 9 de Paus, 10 de Paus, Valete de Paus, ");
		retorno.append("Dama de Paus, Rei de Paus, ");
		return retorno.toString();
	}

}
